#!/usr/bin/ruby

RAILS_ROOT = File.expand_path(File.join(File.dirname(__FILE__), '../..'))


require "RAILS_ROOT/app/models/raxml.rb"

class CheckJobs 

  def initialize
  end

  def run

    raxmls = Raxml.find :all , :conditions => {:email => 'Denis.Krompass@campus.lmu.de'}
    raxmls.each  {|r| puts r.id}
  
  end
end

CheckJobs.new.run
