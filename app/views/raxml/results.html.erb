<div id="sidebar">
  <div id="sidebar_inner">
    <div id="nav">
      
      <li><%= link_to "Home", :action => 'home'%></li>
      <li><%= link_to "Submit job", :action => 'index'%></li>
      <li><%= link_to "Look up jobs", :action => 'look'%></li>
      <li<%= link_to "Contact", :action => 'contact'%></li>
      <li><%= link_to "About", :action => 'about'%></li>
      
    </div>
    <div id="spacer">
    </div>
    <div id="counter">
      <ul>
        <li>Visitors: <%= @ip_counter %></li>
        <li>Submissions: <%= @submission_counter %></li>
      </ul>
    </div>
    <div id="jobs">
    </div>
  </div>
</div>
<div id="content">
  <div id="content_inner">

    <h1>Results</h1>
    
    <h2>Downloads</h2>
    <% i = 0; while i<@names.size %>
       <%= link_to "#{@names[i]}", :action => 'download' , :file => "#{@files[i]}"%><br>
    <% i = i+1; end %>
    <br>
    <%= button_to "Submit new job" , :controller => 'raxml', :action => 'index' %><br>
    
    
    <APPLET codebase="/applets/archaeopteryx" archive="archaeopteryx_applets.jar" code="org.forester.archaeopteryx.ArchaeopteryxE" width="1000" height="700">
      <!--<APPLET  code="org.forester.archaeopteryx.ArchaeopteryxE" name="ArchaeopteryxE" archiv="archaeopteryx_applets.jar" width="1000" height="700">-->
      <param name="url_of_tree_to_load" value= '<%="#{@root}#{@path}treefile.phyloxml"%>' >
      <param name="config_file" value='<%="#{@root}/applets/archaeopteryx/_aptx_configuration_file"%>'>
      
      
    </APPLET>
  </div>
</div>
    
